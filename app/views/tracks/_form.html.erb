    <p> form is here </p>
<%= form_with do |form| %>

    <div>
        <%= form.label :title, style: "display: block" %>
        <%= form.text_field :title %>
    </div>

    <div>
        <%= form.label :file, style: "display: block" %>
        <%= form.file_field :file %>
    </div>

    <div>
        <%= form.submit %>
    </div>

    <% end %>